<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.itb13.oculus.presentation.PatientSearchView">
   <children>
      <Pane layoutY="-1.0" prefHeight="40.0" prefWidth="550.0">
         <children>
            <TextField fx:id="_patientsearchInput" layoutX="122.0" layoutY="8.0" prefHeight="26.0" prefWidth="333.0" />
            <Button fx:id="_patientsearchButton" layoutX="465.0" layoutY="8.0" mnemonicParsing="false" onAction="#showAllPatientsInTable" prefHeight="25.0" prefWidth="81.0" text="Search">
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding></Button>
            <Label fx:id="_searchTermLbl" layoutY="12.0" text="\\%searchTermLbl">
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </Label>
         </children>
      </Pane>
      <ScrollPane layoutY="40.0" minHeight="400.0" minWidth="550.0" prefViewportHeight="400.0" prefViewportWidth="550.0">
         <content>
            <TableView fx:id="_tableView" editable="true" layoutY="40.0" minHeight="400.0" minWidth="550.0" tableMenuButtonVisible="true">
              <columns>
                <TableColumn fx:id="_firstname" prefWidth="94.0" text="%firstname" />
                <TableColumn fx:id="_lastname" prefWidth="91.0" text="%lastname" />
                  <TableColumn fx:id="_address" prefWidth="100.0" text="%address" />
                  <TableColumn fx:id="_socialsecuritynumber" prefWidth="168.0" text="%socialsecuritynumber" />
              </columns>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <effect>
                  <Blend />
               </effect>
            </TableView>
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
