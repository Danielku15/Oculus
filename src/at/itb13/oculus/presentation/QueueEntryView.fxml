<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="259.0" prefWidth="539.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.itb13.oculus.presentation.QueueEntryViewController">
   <children>
      <GridPane layoutX="26.0" layoutY="26.0" prefHeight="193.0" prefWidth="434.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="194.0" minWidth="10.0" prefWidth="149.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="248.0" minWidth="10.0" prefWidth="248.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="%patient" />
            <TextField fx:id="_patientTbx" disable="true" GridPane.columnIndex="1" />
            <Label text="%appointment" GridPane.rowIndex="1" />
            <ChoiceBox fx:id="_appointmentCbx" disable="true" prefHeight="25.0" prefWidth="253.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <ChoiceBox fx:id="_queueCbx" prefHeight="25.0" prefWidth="253.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label text="%queue" GridPane.rowIndex="2" />
            <VBox alignment="CENTER_LEFT" prefHeight="28.0" prefWidth="186.0" GridPane.rowIndex="3">
               <children>
                  <Button fx:id="cancelBtn" alignment="CENTER_RIGHT" contentDisplay="TOP" mnemonicParsing="false" text="%cancel" textAlignment="RIGHT" />
               </children>
            </VBox>
            <VBox alignment="CENTER_RIGHT" prefHeight="15.0" prefWidth="248.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
               <children>
                  <Button fx:id="_okBtn" mnemonicParsing="false" onAction="#addQueueEntry" text="%ok" />
               </children>
            </VBox>
         </children>
      </GridPane>
      <Button fx:id="_searchBtn" layoutX="472.0" layoutY="38.0" mnemonicParsing="false" text="%search" />
   </children>
</AnchorPane>
