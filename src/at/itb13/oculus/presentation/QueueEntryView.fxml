<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="444.0" prefWidth="511.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane layoutX="18.0" layoutY="28.0" prefHeight="417.0" prefWidth="429.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="194.0" minWidth="10.0" prefWidth="103.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="334.0" minWidth="10.0" prefWidth="326.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="90.0" minHeight="0.0" prefHeight="55.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="90.0" minHeight="0.0" prefHeight="55.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="37.0" prefHeight="37.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="88.0" prefHeight="88.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="87.0" minHeight="0.0" prefHeight="47.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="112.0" minHeight="10.0" prefHeight="47.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="%patient" />
            <HBox alignment="CENTER" GridPane.columnIndex="1">
               <children>
                  <TextField fx:id="_patientTbx" disable="false" prefHeight="25.0" prefWidth="286.0" GridPane.columnIndex="1" />
                  <Button fx:id="_searchBtn" mnemonicParsing="false" onAction="#addPatient" prefHeight="25.0" prefWidth="42.0">
                     <graphic>
                        <ImageView fitHeight="18.0" fitWidth="23.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../../../res/search.png" />
                           </image>
                        </ImageView>
                     </graphic></Button>
               </children>
            </HBox>
            <HBox alignment="TOP_CENTER" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <children>
                  <ComboBox fx:id="_appointmentCbx" onAction="#fillAppoinmentData" prefHeight="25.0" prefWidth="338.0" />
               </children>
            </HBox>
            <Label text="%queue" GridPane.rowIndex="5" />
            <VBox alignment="CENTER_LEFT" prefHeight="28.0" prefWidth="186.0" GridPane.rowIndex="3">
               <children>
                  <Label text="%description" />
               </children>
            </VBox>
            <VBox alignment="CENTER_RIGHT" prefHeight="15.0" prefWidth="248.0" GridPane.columnIndex="1" GridPane.rowIndex="5">
               <children>
                  <ComboBox fx:id="_queueCbx" onAction="#addQueueEntry" prefHeight="25.0" prefWidth="357.0" />
               </children>
            </VBox>
            <HBox alignment="BOTTOM_LEFT" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="6">
               <children>
                  <Button fx:id="_cancelBtn" alignment="BOTTOM_LEFT" contentDisplay="TOP" mnemonicParsing="false" text="%cancel" textAlignment="RIGHT" />
               </children>
            </HBox>
            <HBox alignment="BOTTOM_RIGHT" prefHeight="59.0" prefWidth="314.0" GridPane.columnIndex="1" GridPane.rowIndex="6">
               <children>
                  <Button fx:id="_saveBtn" alignment="BOTTOM_RIGHT" mnemonicParsing="false" onAction="#save" text="%save" />
               </children>
            </HBox>
            <Label text="%employee" GridPane.rowIndex="4" />
            <Label text="%title" GridPane.rowIndex="2" />
            <HBox alignment="CENTER_RIGHT" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <children>
                  <TextField fx:id="_titleTbx" editable="false" prefHeight="25.0" prefWidth="350.0" />
               </children>
            </HBox>
            <HBox alignment="BOTTOM_CENTER" GridPane.columnIndex="1" GridPane.rowIndex="3">
               <children>
                  <TextArea fx:id="_descriptionTax" editable="false" prefHeight="1.0" prefWidth="326.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="52.0" prefWidth="314.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
               <children>
                  <TextField fx:id="_employeeTbx" editable="false" prefHeight="25.0" prefWidth="322.0" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="1">
               <children>
                  <Label alignment="TOP_LEFT" text="%appointment" />
               </children>
            </HBox>
         </children>
      </GridPane>
   </children>
</AnchorPane>
