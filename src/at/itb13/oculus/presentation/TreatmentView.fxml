					AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
					AnchorPane.topAnchor="0.0">
					<content>
						<AnchorPane>
							<children>
								<GridPane hgap="20.0" prefHeight="153.0" prefWidth="867.0"	vgap="10.0">
									<columnConstraints>
										<ColumnConstraints hgrow="SOMETIMES"
											maxWidth="160.0" minWidth="160.0" prefWidth="160.0" />
										<ColumnConstraints halignment="LEFT" hgrow="ALWAYS"
											minWidth="100.0" />
										<ColumnConstraints halignment="LEFT" hgrow="ALWAYS"
											maxWidth="40.0" minWidth="40.0" prefWidth="40.0" />
										<ColumnConstraints hgrow="SOMETIMES"
											maxWidth="100.0" minWidth="100.0" prefWidth="100.0" />
										<ColumnConstraints halignment="LEFT" hgrow="ALWAYS"
											minWidth="100.0" />
									</columnConstraints>
									<rowConstraints>
										<RowConstraints maxHeight="50.0" minHeight="10.0"
											prefHeight="50.0" vgrow="SOMETIMES" />
										<RowConstraints maxHeight="50.0" minHeight="10.0"
											prefHeight="50.0" vgrow="SOMETIMES" />
									</rowConstraints>
									<children>
										<Label fx:id="_firstnameLabel" text="%firstname"
											GridPane.rowIndex="1" translateX="10.0">
											<font>
												<Font size="14.0" />
											</font>
										</Label>
										<Label fx:id="_lastnameLabel" text="%lastname"
											GridPane.columnIndex="3" GridPane.rowIndex="1">
											<font>
												<Font size="14.0" />
											</font>
										</Label>
										<Label fx:id="_socialSecurityNumberLabel" text="%socialsecuritynumber" translateX="10.0">
											<font>
												<Font size="14.0" />
											</font>
										</Label>
										<Label fx:id="_firstnameLoadedLabel"
											GridPane.columnIndex="1" GridPane.rowIndex="1" />
										<Label fx:id="_lastnameLoadedLabel"
											GridPane.columnIndex="4" GridPane.rowIndex="1" />
										<Label fx:id="_socialSecurityLoadedNumberLabel"
											GridPane.columnIndex="1" />
									</children>
								</GridPane>
								<Accordion fx:id="_accordion" layoutY="127.0"
									minWidth="520.0" prefHeight="871.0" prefWidth="898.0"
									AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
									AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="127.0">
									<panes>
										<TitledPane animated="false" minWidth="520.0"
											text="%measurement">
											<content>
												<AnchorPane />
											</content>
											<font>
												<Font size="18.0" />
											</font>
										</TitledPane>
									</panes>
								</Accordion>
							</children>
							<opaqueInsets>
								<Insets />
							</opaqueInsets>
						</AnchorPane>
					</content>
				</ScrollPane>
			</children>
		</AnchorPane>
	</children>
